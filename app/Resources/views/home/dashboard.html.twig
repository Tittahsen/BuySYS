{% extends 'secure_base.html.twig' %}{% block body %}    <section class="content-header">        <h1>            {{ title }}            <small>{{ subtitle }}</small>        </h1>    </section>    <section class="content">        <div class="row">            <div class="col-md-9">                <div class="box">                    <div class="box-header with-border">                        <h3 class="box-title"><i class="fa fa-money "></i> Selling</h3>                    </div>                    <div class="box-body">                        <div class="row">                            <div class="col-md-12">                                <div class="alert alert-error alert-dismissable" id="more-than-500"                                     style="display:none;">                                    <h4 style="margin-bottom:0px;"><i class="icon fa fa-times"></i> Contracts are                                        limited to 500 items or less!</h4>                                </div>                                <form method="post" action="{{ path('estimate') }}" id="estimate-form">                                    <textarea class="form-control" rows="25" name="items" id="estimate-items"></textarea>                                </form>                            </div>                        </div>                    </div>                </div>            </div>            <div class="col-md-3">                <div class="box box-solid">                    <div class="box-body">                        <button class="form-control btn-block btn-success" id="get-estimate-button">Get Estimate</button>                    </div>                </div>                <div class="info-box small">                    <span class="info-box-icon bg-red"><i class="fa fa-money"></i></span>                    <div class="info-box-content">                        <span class="info-box-text">Pending Income</span>                        <span class="info-box-number"                              id="pendingIncome">{{ pendingIncome|number_format(0, '.', ',') }}</span>                        <span class="text-muted"></span>                    </div><!-- /.info-box-content -->                </div>                <div class="info-box small">                    <span class="info-box-icon bg-green"><i class="fa fa-money"></i></span>                    <div class="info-box-content">                        <span class="info-box-text">Completed Income</span>                        <span class="info-box-number"                              id="completedIncome">{{ actualIncome|number_format(0, '.', ',') }}</span>                        <span class="text-muted"></span>                    </div><!-- /.info-box-content -->                </div>                <div class="box box-solid">                    <div class="box-body">                        <strong>What can I paste?</strong>                        <p>You can copy and paste from the following locations:</p>                        <ul>                            <li>Inventory (Any view)</li>                            <li>Contracts</li>                            <li>Remote Viewing of Can in Station</li>                        </ul>                    </div>                </div>            </div>        </div>        </div>        <div class="modal fade" id="index_modal">            <div class="modal-dialog">                <div class="modal-content" id="index_modal_content">                </div><!-- /.modal-content -->            </div><!-- /.modal-dialog -->        </div><!-- /.modal -->        <div class="modal fade" id="estimate_modal">            <div class="modal-dialog" id="estimate_modal_content">                <div class="modal-content">                    <div class="modal-loading"><i class="fa fa-refresh fa-spin"></i></div>                </div><!-- /.modal-content -->            </div><!-- /.modal-dialog -->        </div><!-- /.modal -->        </div>    </section>{% endblock %}{% block stylesheets %}{% endblock %}{% block javascripts %}    <script type="text/javascript">        $(document).ready(function (e) {            $(function () {                $('[data-toggle="tooltip"]').tooltip()            })            $('#get-estimate-button').click(function() {                $('#estimate-form').submit();            });            $("#estimate-items").bind("paste", function (e) {                var pData = e.originalEvent.clipboardData.getData('text');                var numberOfLineBreaks = (pData.match(/\n/g) || []).length;                if (numberOfLineBreaks > 500) {                    $("#more-than-500").css("display", "block");                    e.preventDefault();                } else {                    $("#more-than-500").css("display", "none");                }            });        });    </script>{% endblock %}